          "_name": "features2",
          "_customHTML": "<section class=\"features2\" group=\"Features\" plugins=\"amp-main\">\n    <mbr-parameters>\n        <input type=\"range\" inline=\"\" title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n        <input type=\"range\" inline=\"\" title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\">\n            <header>Cards</header>\n        <select title=\"CardsAmount\" name=\"cardsAmount\">\n            <option value=\"1\">1</option>\n            <option value=\"2\" selected=\"\">2</option>\n            <option value=\"3\">3</option>\n            <option value=\"4\">4</option>\n        </select>\n        <input type=\"checkbox\" title=\"Transparent Card\" name=\"transparentCard\">\n        <input type=\"color\" title=\"Card Background\" name=\"cardBG\" value=\"#ffffff\" condition=\"!transparentCard\">\n        <header>Cards Text</header>\n        <input type=\"checkbox\" title=\"Show Title\" name=\"showCardsTitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Show Text\" name=\"showCardsText\" checked=\"\">\n        <input type=\"checkbox\" title=\"Show Buttons\" name=\"showCardsButtons\">\n        <header>Cards Image</header>\n        <input type=\"checkbox\" title=\"Show Image\" name=\"showImg\" checked=\"\">\n        <input type=\"range\" inline=\"\" title=\"Image Height\" name=\"imgHeight\" min=\"200\" max=\"600\" step=\"10\" value=\"200\" condition=\"showImg\">\n        <input type=\"checkbox\" title=\"Image Effect\" name=\"imgEffect\" condition=\"showImg\" checked=\"\">\n        <input type=\"color\" title=\"Color 1\" name=\"imgOverlayColor1\" value=\"#3b7eff\" condition=\"((showImgIcon &amp;&amp; !transparentCard) || imgEffect) &amp;&amp; cardsAmount >=1 &amp;&amp; showImg\">\n        <input type=\"color\" title=\"Color 2\" name=\"imgOverlayColor2\" value=\"#72a0fb\" condition=\"((showImgIcon &amp;&amp; !transparentCard) || imgEffect) &amp;&amp; cardsAmount >=2 &amp;&amp; showImg\">\n        <input type=\"color\" title=\"Color 3\" name=\"imgOverlayColor3\" value=\"#7d3ed5\" condition=\"((showImgIcon &amp;&amp; !transparentCard) || imgEffect) &amp;&amp; cardsAmount >=3 &amp;&amp; showImg\">\n        <input type=\"color\" title=\"Color 4\" name=\"imgOverlayColor4\" value=\"#21ac13\" condition=\"((showImgIcon &amp;&amp; !transparentCard) || imgEffect) &amp;&amp; cardsAmount >=4 &amp;&amp; showImg\">\n        <input type=\"range\" inline=\"\" title=\"Image Overlay Opacity\" name=\"imgOverlayOpacity\" min=\"0\" max=\"0.9\" step=\"0.05\" value=\"0.3\" condition=\"showImg &amp;&amp; imgEffect\">\n        <input type=\"checkbox\" title=\"Show Image Icon\" name=\"showImgIcon\" condition=\"showImg &amp;&amp; !transparentCard\" checked=\"\">\n        <input type=\"range\" title=\"Icon Size\" name=\"IconSize\" min=\"20\" max=\"40\" step=\"5\" value=\"30\" condition=\"showImg &amp;&amp; showImgIcon &amp;&amp; !transparentCard\">\n        <!--  -->\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\" required=\"\">\n            <input type=\"color\" title=\"Background Color\" value=\"#ffffff\" selected=\"\">\n            <input type=\"image\" title=\"Background Image\" value=\"../_images/ect24.jpg\">\n        </fieldset>\n        <!--  -->\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked=\"\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#3b7eff\" condition=\"overlay &amp;&amp; bg.type == 'image'\">\n        <input type=\"range\" inline=\"\" title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.3\" condition=\"overlay &amp;&amp; bg.type == 'image'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay &amp;&amp; bg.type !== 'color'\"></div>\n    <div class=\"\" mbr-class=\"{'container':!fullWi